{
    "name": "diacronia",
    "type": "dark",

    "colors": {
        "activityBar.background": "#1e1717", 
        "activityBar.foreground": "#bababa", 
        "sideBar.background": "#3F3131", 
        "sideBar.foreground": "#f9f9f9",
        "editor.background": "#1e1717", /*"#292a2b",*/
        "editor.foreground": "#f9f9f9",
        "editorLineNumber.foreground": "#bababa",
        "editorGroupHeader.tabsBackground": "#3F3131",
        "editorGroupHeader.tabsBorder": "#1e1717",
        "tab.activeBackground": "#1e1717",
        "tab.activeBorder": "#bababa",
        "tab.inactiveBorder": "#bababa",
        "tab.inactiveBackground": "#3F3131",
        "tab.inactiveForeground": "#bababa",
        "statusBar.background": "#3F3131", /* #D8CBC7 */
        "statusBar.foreground": "#bababa",
        "titleBar.activeBackground": "#1e1717"
    },

    "tokenColors": [
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#FF9999",
                "fontStyle": "italic"
            }
        },       
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],          
            "settings": {
                "foreground": "#43494C",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Keyword Operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.new.js",
                "keyword.operator.js"
            ],
            "settings": {
                "foreground": "#FFCF99" /*"#93FFF7"*/
            }
        },
        {
            "name": "Variable Language Operators",
            "scope": [
                "variable.language",
                "keyword.control"
            ],
            "settings": {
                "foreground": "#9C528B"
            }
        },
        {
            "name": "Storage Type && Constant Numeric",
            "scope": [
                "storage.type",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#D7BEA8"
            }
        },
        {
            "name": "Support",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#FFCF99"
            }
        }
    ]
    
}