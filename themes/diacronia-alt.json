        "inputValidation.infoBorder": "#B25358",

        "terminal.background": "#141616",
        "terminal.foreground": "#D2D2E6",
        "terminal.ansiBlack": "#141616",
        "terminal.ansiBlue": "#5e9ccf",
        "terminal.ansiBrightBlack": "#525A67",
        "terminal.ansiBrightBlue": "#4583B6",
        "terminal.ansiBrightCyan": "#389DAA",
        "terminal.ansiBrightGreen": "#51AA71",
        "terminal.ansiBrightMagenta": "#C64E06",
        "terminal.ansiBrightRed": "#C9525A",
        "terminal.ansiBrightWhite": "#D2D2E6",
        "terminal.ansiBrightYellow": "#CDA85D",
        "terminal.ansiCyan": "#51b6c3",
        "terminal.ansiGreen": "#6ac38a",
        "terminal.ansiMagenta": "#df671f",
        "terminal.ansiRed": "#e26b73",
        "terminal.ansiWhite": "#ebebff",
        "terminal.ansiYellow": "#e6c176",
        "terminalCursor.foreground": "#D2D2E6"
    },

    "tokenColors": [
        {
            "name": "Orange (string)",
            "scope": [
                "string",
                "support.constant.property-value.css",
                "support.constant",
                "support.type",
                "support.class",
                "support.other.variable"
            ],
            "settings": {
                "foreground": "#AE8478"
            }
        },
        {
            "name": "Bold and grey (comments)",
            "scope": [
                "comment",
                "invalid",
                "invalid.deprecated"
            ],          
            "settings": {
                "foreground": "#43494C",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Rose (Operators and new)",
            "scope": [
                "keyword",
                "keyword.operator",
                "keyword.operator.new.js",
                "keyword.operator.js",
                "entity.other.attribute-name.html",
                "entity.name.class",
                "entity.name.function",
                "entity.other.inherited-class",
                "support"
            ],
            "settings": {
                "foreground": "#DDBAA2"
            }
        },
        {
            "name": "Rose (var e const)",
            "scope": [
                "storage",
                "storage.type",
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#BF959B"
            }
        },
        {
            "name": "Violet (this e keyword)",
            "scope": [
                "keyword.control",
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#CC6E8A"
            }
        },
        {
            "name": "Dark red",
            "scope": [
                "entity.name.class",
                "entity.name.struct",
                "entity.name.enum",
                "entity.name.union",
                "entity.name.trait",
                "entity.name.interface",
                "entity.name.type",
                "entity.other.attribute-name",
                "support.function"
            ],
            "settings": {
                "foreground": "#B25358"
            }
        },
        {
            "name": "Brown",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#7A8687",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "White",
            "scope": [
                "variable",
                "variable.language"
            ],
            "settings": {
                "foreground": "#E5E3E3"
            }
        }
    ]
    
}